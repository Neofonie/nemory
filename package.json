    "autoprefixer": "^10.3.3",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.17.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "sass": "^1.38.2",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-standard": "^22.0.0",
    "tailwindcss": "^2.2.9",
    "vite": "^2.5.2"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "maintained node versions",
