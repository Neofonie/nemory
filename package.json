    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "postcss": "^8.4.12",
    "prettier": "^2.6.0",
    "sass": "^1.49.9",
    "stylelint": "^14.6.0",
    "stylelint-config-recommended-vue": "^1.3.0",
